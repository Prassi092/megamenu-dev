<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Global Header"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <tabs
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/tabs"
                maximized="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <general
                        jcr:primaryType="nt:unstructured"
                        jcr:title="General"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <color-scheme
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Portal Color Scheme"
                                                name="./colorScheme">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <left
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Green"
                                                        value="member"/>
                                                    <center
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Orange"
                                                        value="employer"/>
                                                </items>
                                            </color-scheme>
                                            <portal-config
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                class="full-width"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add Member/Employer link"
                                                fieldLabel="Portal URLs">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                    name="./portalLinks">
                                                    <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                        method="absolute"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <links
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <title
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Portal Label"
                                                                    name="./linkLabel"/>
                                                                <path
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Portal Path"
                                                                    name="./linkPath"
                                                                    rootPath="/content"
                                                                    validation="non-external-url"/>
                                                                <openInNewWindow
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                                    fieldLabel="Open in new tab"
                                                                    name="./linkOpenInNewWindow"
                                                                    text="Open in new tab"
                                                                    value="true"/>
                                                            </items>
                                                        </links>
                                                    </items>
                                                </field>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    max="2"/>
                                            </portal-config>
                                            <login-option
                                                granite:class="cq-dialog-dropdown-showhide"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/select"
                                                fieldLabel="Login Option"
                                                name="./loginOption">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <loginpage
                                                        jcr:primaryType="nt:unstructured"
                                                        selected="{Boolean}true"
                                                        text="Go to login screen"
                                                        value="loginpage"/>
                                                    <otherpage
                                                        jcr:primaryType="nt:unstructured"
                                                        text="Go to another page"
                                                        value="otherpage"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    cq-dialog-dropdown-showhide-target=".showhideloginoptiontarget"/>
                                            </login-option>
                                            <login-option-target
                                                granite:class="showhideloginoptiontarget"
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                <items jcr:primaryType="nt:unstructured">
                                                    <login-url
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                        fieldLabel="Another Page Path"
                                                        name="./loginUrl"
                                                        rootPath="/content/web/sg/en"/>
                                                </items>
                                                <granite:data
                                                    jcr:primaryType="nt:unstructured"
                                                    showhidetargetvalue="otherpage"/>
                                            </login-option-target>
                                            <search-page
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Search Page URL"
                                                name="./searchPath"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <search-result-icon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Refer to list of icon library in Storybook (i.e.: icon-houseownership)"
                                                fieldLabel="Search Result Icon"
                                                name="./searchResultIcon"/>
                                            <megamenu-links-anchor
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Override the root path used to generate level 1 links in the header. Used only if component is used in an Experience Fragment"
                                                fieldLabel="Navigation Root Path"
                                                name="./megamenu"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <session-validation-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Page URL to redirect user to confirm before invalidating the session."
                                                fieldLabel="Session Validation Page Path"
                                                name="./sessionValidationPath"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </general>
                    <logo
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Logo Config"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <logo-image
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                                                allowUpload="false"
                                                autoStart="{Boolean}false"
                                                class="cq-droptarget"
                                                fieldLabel="Logo"
                                                fileNameParameter="./fileName"
                                                fileReferenceParameter="./logoImage"
                                                mimeTypes="[image/gif,image/jpeg,image/png,image/tiff,image/svg+xml]"
                                                multiple="{Boolean}false"
                                                name="./logo"
                                                required="{Boolean}true"
                                                title="Add Logo"
                                                uploadUrl="${suffix.path}"
                                                useHTML5="{Boolean}true"/>
                                            <logo-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Logo URL"
                                                name="./logoURL"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <logo-alt-text
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldLabel="Logo Alternative Text"
                                                name="./logoAlt"/>
                                            <open-in-new-tab
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                fieldDescription="Keep default selection for CPF Webpages to open in same tab."
                                                fieldLabel="Open in new tab"
                                                name="./openInNewTab"
                                                text="Open in new tab"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </logo>
                    <level-1-links
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Level 1 Links"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <level-1-links-config
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                class="full-width"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add new link"
                                                fieldLabel="Level 1 URLs">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                    name="./level1Links">
                                                    <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                        method="absolute"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <links
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <title
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Link Label"
                                                                    name="./linkLabel"/>
                                                                <path
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Link Path"
                                                                    name="./linkPath"
                                                                    rootPath="/content"
                                                                    validation="non-external-url"/>
                                                                <openInNewWindow
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                                    fieldLabel="Open in new tab"
                                                                    name="./linkOpenInNewWindow"
                                                                    text="Open in new tab"
                                                                    value="true"/>
                                                            </items>
                                                        </links>
                                                    </items>
                                                </field>
                                            </level-1-links-config>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </level-1-links>
                    <quick-acion
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Quick Actions"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <quick-action-config
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                class="full-width"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add new link"
                                                fieldLabel="Quick Actions">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                    name="./quickActions">
                                                    <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                        method="absolute"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <links
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <title
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Link Label"
                                                                    name="./linkLabel"/>
                                                                <path
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Link Path"
                                                                    name="./linkPath"
                                                                    rootPath="/content"/>
                                                                <icon
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Refer to list of icon library in Storybook"
                                                                    fieldLabel="Quick Action Icon"
                                                                    name="./icon"/>
                                                                <openInNewWindow
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                                    fieldDescription="Keep default selection for CPF Webpages to open in same tab."
                                                                    fieldLabel="Open in new tab"
                                                                    name="./linkOpenInNewWindow"
                                                                    text="Open in new tab"
                                                                    value="true"/>
                                                            </items>
                                                        </links>
                                                    </items>
                                                </field>
                                            </quick-action-config>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </quick-acion>
                    <post-login
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Post Login"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <my-cpf
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Source path to retrieve list of dashboard and statements from"
                                                fieldLabel="My CPF Root Path"
                                                name="./eServicesLink"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <icon
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Refer to list of icon library in Storybook"
                                                fieldLabel="Account Settings Icon"
                                                name="./entryPointIcon"/>
                                            <label
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Label will appear in the dropdown for header in Post Login state"
                                                fieldLabel="Account Settings Label"
                                                name="./entryPointLabel"/>
                                            <link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Account Settings Link"
                                                name="./entryPointLink"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <logout
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldLabel="Log out Link"
                                                name="./logoutLink"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <my-cpf-home-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="My CPF Home URL"
                                                fieldLabel="My CPF Home Link"
                                                name="./myCpfHomeUrl"
                                                rootPath="/content"
                                                validation="non-external-url"/>
                                            <my-cpf-description
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                fieldDescription="Aditional description for my cpf to provide extra context. This treatment is only applicable to the post-login menu item my cpf."
                                                fieldLabel="My CPF Description"
                                                name="./myCpfDescription"/>
                                            <quick-action-config
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                class="full-width"
                                                composite="{Boolean}true"
                                                fieldDescription="Click 'Add' to add new link"
                                                fieldLabel="Quick Actions">
                                                <field
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/foundation/form/fieldset"
                                                    name="./postLoginQuickActions">
                                                    <layout
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"
                                                        method="absolute"/>
                                                    <items jcr:primaryType="nt:unstructured">
                                                        <links
                                                            jcr:primaryType="nt:unstructured"
                                                            sling:resourceType="granite/ui/components/foundation/container">
                                                            <items jcr:primaryType="nt:unstructured">
                                                                <title
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldLabel="Link Label"
                                                                    name="./linkLabel"/>
                                                                <path
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                                    fieldLabel="Link Path"
                                                                    name="./linkPath"
                                                                    rootPath="/content"/>
                                                                <icon
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                    fieldDescription="Refer to list of icon library in Storybook"
                                                                    fieldLabel="Quick Action Icon"
                                                                    name="./icon"/>
                                                                <openInNewWindow
                                                                    jcr:primaryType="nt:unstructured"
                                                                    sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                                    fieldDescription="Keep default selection for CPF Webpages to open in same tab."
                                                                    fieldLabel="Open in new tab"
                                                                    name="./linkOpenInNewWindow"
                                                                    text="Open in new tab"
                                                                    value="true"/>
                                                            </items>
                                                        </links>
                                                    </items>
                                                </field>
                                            </quick-action-config>
                                            <showPostLoginHeader
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/switch"
                                                fieldDescription="To see post login header, please check this toggle. If not selected will always show pre-login header even if the user is logged in"
                                                fieldLabel="Show post-login header"
                                                name="./showPostLoginHeader"
                                                text="Show post-login header"
                                                value="true"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </post-login>
                    <analytics
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/foundation/include"
                        path="web/components/common/tabs/analytics"/>
                    <inherited-page-properties
                        jcr:primaryType="nt:unstructured"
                        jcr:title="Inherited Page Properties"
                        sling:resourceType="granite/ui/components/coral/foundation/container"
                        margin="{Boolean}true">
                        <items jcr:primaryType="nt:unstructured">
                            <columns
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                                margin="{Boolean}true">
                                <items jcr:primaryType="nt:unstructured">
                                    <column
                                        jcr:primaryType="nt:unstructured"
                                        sling:resourceType="granite/ui/components/coral/foundation/container">
                                        <items jcr:primaryType="nt:unstructured">
                                            <my-dashboards-page-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Set the url for Header component to show Dashboards in Mega Menu"
                                                fieldLabel="Dashboards' Root Page"
                                                name="./myDashboardsUrl"
                                                rootPath="/content"/>
                                            <my-statement-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Define the root path for Header component to show Statements in Mega Menu"
                                                fieldLabel="Statements' Root Page"
                                                name="./myStatementsUrl"
                                                rootPath="/content"/>
                                            <child-dashboard-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Define the url for the Child Dashboard Page. Used by Header component to hide link when it is not applicable for certain member profile"
                                                fieldLabel="Child Dashboard Page"
                                                name="./childDashboardUrl"
                                                rootPath="/content"/>
                                            <education-dashboard-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Define the url for the Education Dashboard Page. Used by Header component to hide link when it is not applicable for certain member profile"
                                                fieldLabel="Education Dashboard Page"
                                                name="./educationDashboardUrl"
                                                rootPath="/content"/>
                                            <sep-dashboard-url
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="This url will be pulled from Inherited page properties before fallback this value for the Self Employed Dashboard Page. Used by Header component to hide link when it is not applicable for certain member profile"
                                                fieldLabel="Self Employed Dashboard Page"
                                                name="./sepDashboardUrl"
                                                rootPath="/content"/>
                                            <my-activities-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Define the url for My Activities. Used by Header component to hide link when it is not applicable for certain member profile"
                                                fieldLabel="My Activites Link"
                                                name="./myActivitiesLink"
                                                rootPath="/content"/>
                                            <my-requests-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Define the url for My Requests. Used by Header component to hide link when it is not applicable for certain member profile"
                                                fieldLabel="My Requests Link"
                                                name="./myRequestsLink"
                                                rootPath="/content"/>
                                            <my-mailbox-link
                                                jcr:primaryType="nt:unstructured"
                                                sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                                                fieldDescription="Define the url for My Mailbox. Used by Header component to hide link when it is not applicable for certain member profile"
                                                fieldLabel="My Mailbox Link"
                                                name="./myMailboxLink"
                                                rootPath="/content"/>
                                        </items>
                                    </column>
                                </items>
                            </columns>
                        </items>
                    </inherited-page-properties>
                </items>
            </tabs>
        </items>
    </content>
</jcr:root>
